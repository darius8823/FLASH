<div class="container">
    <%= form_for( @event ) do |f| %>
    <%= render partial: 'form', locals: {f:f} %>

    <div class="btn-group">
        <%= link_to "Back", events_path, class: "btn btn-secondary" %>
        <%= f.submit "Create Event", disable_with: 'Creating Event...', class: "btn btn-success" %>
    </div>
    
    <% end %>
</div>